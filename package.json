{
  "name": "bear-graphql-code-generator",
  "description": "Dropdown list with React Component",
  "keywords": [
    "typescript",
    "graphql-code-generator",
    "react-query"
  ],
  "private": false,
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build:claer": "rm -rf ./dist",
    "prepublishOnly": "run-s build",
    "build": "run-s build:claer && tsc --project ./tsconfig.build.json",
    "lint:fix": "eslint ./src --fix",
    "cz": "git-cz",
    "test": "jest",
    "gitlog": "git log --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cblueby %an %Cgreen(%cr)%Creset'",
    "release": "standard-version --release-as",
    "release:major": "standard-version -r major",
    "release:minor": "standard-version -r minor",
    "release:patch": "standard-version -r patch",
    "release:alpha": "standard-version --prerelease alpha",
    "postrelease": "# then run `git push --follow-tags origin main && yarn build && npm publish ./src/react-query --access=public` to publish"
  },
  "peerDependencies": {
    "graphql": "^0.8.0 || ^0.9.0 || ^0.10.0 || ^0.11.0 || ^0.12.0 || ^0.13.0 || ^14.0.0 || ^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "@graphql-tools/apollo-engine-loader": "^7.3.26",
    "graphql": "^16.6.0"
  },
  "devDependencies": {
    "@graphql-codegen/plugin-helpers": "^3.0.0",
    "@graphql-codegen/visitor-plugin-common": "2.13.1",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "auto-bind": "~4.0.0",
    "change-case-all": "1.0.15",
    "cz-git": "^1.6.1",
    "eslint": "^8.37.0",
    "jest": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "rollup-plugin-visualizer": "^5.9.0",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.0",
    "tslib": "~2.4.0",
    "typescript": "^4.9.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
