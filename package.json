{
  "name": "bear-graphql-react-query",
  "version": "3.0.1-alpha.1",
  "description": "Dropdown list with React Component",
  "keywords": [
    "typescript",
    "graphql-react-query"
  ],
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/imagine10255/bear-graphql-react-query.git"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "dev": "vite build -w",
    "build:claer": "rm -rf ./dist",
    "prepublishOnly": "run-s build",
    "build": "run-s build:claer && tsc --project ./tsconfig.build.json",
    "build2": "run-s build:claer && tsc && vite build",
    "preview": "vite preview",
    "lint:fix": "eslint ./src --fix",
    "cz": "git-cz",
    "test": "jest",
    "gitlog": "git log --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cblueby %an %Cgreen(%cr)%Creset'",
    "release": "standard-version --release-as",
    "release:major": "standard-version -r major",
    "release:minor": "standard-version -r minor",
    "release:patch": "standard-version -r patch",
    "release:alpha": "standard-version --prerelease alpha"
  },
  "dependencies": {
    "@graphql-codegen/typescript-react-query": "^4.x",
    "@graphql-codegen/visitor-plugin-common": "^3.x",
    "@graphql-tools/apollo-engine-loader": "^7.3.26",
    "graphql": "^16.x"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "cz-git": "^1.6.1",
    "eslint": "^8.37.0",
    "jest": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "rollup-plugin-visualizer": "^5.9.0",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-plugin-dts": "^2.1.0",
    "vite-plugin-eslint": "^1.8.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
